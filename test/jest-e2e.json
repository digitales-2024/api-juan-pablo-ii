    "@pacient/pacient": "<rootDir>/../libs/pacient/src",
    "@schedule/schedule/(.*)": "<rootDir>/../libs/schedule/src/$1",
    "@schedule/schedule": "<rootDir>/../libs/schedule/src",
    "@app/calendar/(.*)": "<rootDir>/../libs/calendar/src/$1",
