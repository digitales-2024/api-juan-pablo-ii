    "@pay/pay": "<rootDir>/../libs/pay/src",
    "@prescription/prescription/(.*)": "<rootDir>/../libs/prescription/src/$1",
    "@prescription/prescription": "<rootDir>/../libs/prescription/src",
    "@doc-appointment/doc-appointment/(.*)": "<rootDir>/../libs/doc-appointment/src/$1",
