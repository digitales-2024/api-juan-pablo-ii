{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "forceConsistentCasingInFileNames": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "jsx": "react-jsx",
    "jsxImportSource": "@kitajs/html",
    "plugins": [
      {
        "name": "@kitajs/ts-html-plugin"
      }
    ],
    "paths": {
      "@login/login": [
        "libs/login/src"
      ],
      "@login/login/*": [
        "libs/login/src/*"
      ],
      "@prisma/prisma": [
        "libs/prisma/src"
      ],
      "@prisma/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@clients/clients": [
        "libs/clients/src"
      ],
      "@clients/clients/*": [
        "libs/clients/src/*"
      ],
      "@inventory/inventory": [
        "libs/inventory/src"
      ],
      "@inventory/inventory/*": [
        "libs/inventory/src/*"
      ],
      "@pay/pay": [
        "libs/pay/src"
      ],
      "@pay/pay/*": [
        "libs/pay/src/*"
      ],
      "@prescription/prescription": [
        "libs/prescription/src"
      ],
      "@prescription/prescription/*": [
        "libs/prescription/src/*"
      ],
      "@doc-appointment/doc-appointment": [
        "libs/doc-appointment/src"
      ],
      "@doc-appointment/doc-appointment/*": [
        "libs/doc-appointment/src/*"
      ],
      "@working-hours/working-hours": [
        "libs/working-hours/src"
      ],
      "@working-hours/working-hours/*": [
        "libs/working-hours/src/*"
      ],
      "@medical-procedure/medical-procedure": [
        "libs/medical-procedure/src"
      ],
      "@medical-procedure/medical-procedure/*": [
        "libs/medical-procedure/src/*"
      ],
      "@medical-consultation/medical-consultation": [
        "libs/medical-consultation/src"
      ],
      "@medical-consultation/medical-consultation/*": [
        "libs/medical-consultation/src/*"
      ],
      "@pacient/pacient": [
        "libs/pacient/src"
      ],
      "@pacient/pacient/*": [
        "libs/pacient/src/*"
      ],
      "@schedule/schedule": [
        "libs/schedule/src"
      ],
      "@schedule/schedule/*": [
        "libs/schedule/src/*"
      ]
    }
  }
}